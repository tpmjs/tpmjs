{
  "name": "@tpmjs/tool-ideas",
  "version": "0.1.0",
  "description": "Generate 10K high-quality AI tool ideas for the TPMJS registry",
  "private": true,
  "type": "module",
  "bin": {
    "tool-ideas": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "cli": "tsx src/cli.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.7",
    "ai": "^6.0.23",
    "better-sqlite3": "^12.5.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "drizzle-orm": "^0.45.1",
    "ora": "^9.0.0",
    "p-limit": "^7.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tpmjs/tsconfig": "workspace:*",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.3",
    "drizzle-kit": "^0.31.8",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
