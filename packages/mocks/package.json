{
  "name": "@tpmjs/mocks",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./handlers": {
      "types": "./dist/handlers.d.ts",
      "default": "./dist/handlers.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "msw": "^2.12.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tpmjs/tsconfig": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
